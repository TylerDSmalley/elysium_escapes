{% extends "admin/master.html.twig" %}

{% block title %}User List{% endblock %}

{% block content %}
<table border="1">
<tr><th>ID</th><th>Name</th><th>Email</th><th>Phone</th><th>User Type</th><th>Actions</th></tr>
{% for u in usersList %}
    <tr><td>{{u.id}}</td><td>{{u.username}}</td><td>{{u.email}}</td><td>{{u.phone}}</td>
    <td>{{u.type}}</td><td><a href="/admin/users/{{u.id}}/edit">Edit</a></td>
    <td><a href="/admin/users/{{u.id}}/delete">Delete</a></td></tr>
{% endblock %}    
</table>
{% endblock %}